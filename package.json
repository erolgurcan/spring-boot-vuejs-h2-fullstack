{
  "name": "mobile-care-app",
  "version": "1.0.0",
  "description": "Mobile web care application",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-full": "npm run install-backend && pm run install-frontend",
    "start-full": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "install-backend": "cd backend && chmod +x mvnw && ./mvnw clean package ",
    "install-frontend": "cd frontend && npm install",
    "start-backend": "cd backend && mvnw spring-boot:run",
    "start-frontend": "cd frontend && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erolgurcan/spring-boot-server-app.git"
  },
